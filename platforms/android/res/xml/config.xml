<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cosoftstudios.UnitConverter" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <name>Basic Unit Converter</name>
    <description>
        A simple unit converting app.
    </description>
    <author email="null" href="">
        LucasGI
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon density="mdpi" src="www/src/ic_launcher/mipmap-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="www/src/ic_launcher/mipmap-hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="www/src/ic_launcher/mipmap-xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="www/src/ic_launcher/mipmap-xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="www/src/ic_launcher/mipmap-xxxhdpi/ic_launcher.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
